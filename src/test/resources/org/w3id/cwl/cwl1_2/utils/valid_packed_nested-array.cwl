{
    "class": "CommandLineTool",
    "baseCommand": "echo",
    "inputs": [
        {
            "type": {
                "type": "array",
                "items": {
                    "type": "array",
                    "items": "string"
                }
            },
            "inputBinding": {
                "position": 1
            },
            "id": "#main/letters"
        }
    ],
    "stdout": "echo.txt",
    "outputs": [
        {
            "type": "File",
            "id": "#main/echo",
            "outputBinding": {
                "glob": "echo.txt"
            }
        }
    ],
    "id": "#main",
    "cwlVersion": "v1.2"
}