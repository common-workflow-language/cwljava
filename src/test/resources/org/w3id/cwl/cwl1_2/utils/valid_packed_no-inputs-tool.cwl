{
    "class": "CommandLineTool",
    "doc": "CommandLineTool without inputs.",
    "hints": [
        {
            "dockerPull": "debian:stretch-slim",
            "class": "DockerRequirement"
        }
    ],
    "inputs": [],
    "outputs": [
        {
            "type": "File",
            "outputBinding": {
                "glob": "output"
            },
            "id": "#main/output"
        }
    ],
    "baseCommand": [
        "echo",
        "cwl"
    ],
    "stdout": "output",
    "id": "#main",
    "cwlVersion": "v1.2"
}