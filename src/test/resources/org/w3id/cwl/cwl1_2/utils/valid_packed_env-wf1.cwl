{
    "$graph": [
        {
            "class": "CommandLineTool",
            "inputs": [
                {
                    "type": "string",
                    "id": "#env-tool1.cwl/in"
                }
            ],
            "outputs": [
                {
                    "type": "File",
                    "outputBinding": {
                        "glob": "out"
                    },
                    "id": "#env-tool1.cwl/out"
                }
            ],
            "requirements": [
                {
                    "envDef": [
                        {
                            "envValue": "$(inputs.in)",
                            "envName": "TEST_ENV"
                        }
                    ],
                    "class": "EnvVarRequirement"
                }
            ],
            "baseCommand": [
                "/bin/sh",
                "-c",
                "echo $TEST_ENV"
            ],
            "stdout": "out",
            "id": "#env-tool1.cwl"
        },
        {
            "class": "Workflow",
            "inputs": [
                {
                    "type": "string",
                    "id": "#main/in"
                }
            ],
            "outputs": [
                {
                    "type": "File",
                    "outputSource": "#main/step1/out",
                    "id": "#main/out"
                }
            ],
            "requirements": [
                {
                    "envDef": [
                        {
                            "envValue": "override",
                            "envName": "TEST_ENV"
                        }
                    ],
                    "class": "EnvVarRequirement"
                }
            ],
            "steps": [
                {
                    "run": "#env-tool1.cwl",
                    "in": [
                        {
                            "source": "#main/in",
                            "id": "#main/step1/in"
                        }
                    ],
                    "out": [
                        "#main/step1/out"
                    ],
                    "id": "#main/step1"
                }
            ],
            "id": "#main"
        }
    ],
    "cwlVersion": "v1.2"
}